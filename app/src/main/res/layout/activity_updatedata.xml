<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4CAF50"
    tools:context=".inputdata">

    <EditText
        android:id="@+id/jeniskelamininput"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="20dp"
        android:ellipsize="none"
        android:ems="10"
        android:inputType="textPersonName"
        android:scrollbarSize="4dp"
        android:text="Jenis Kelamin"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toTopOf="@+id/alamatinput"
        app:layout_constraintEnd_toEndOf="@+id/alamatinput"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView19" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/nomorinput"
        app:layout_constraintEnd_toStartOf="@+id/nomorinput"
        app:layout_constraintTop_toTopOf="@+id/nomorinput"
        app:srcCompat="@drawable/ic_create_black_24dp" />

    <EditText
        android:id="@+id/alamatinput"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="144dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Alamat"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.698"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/namainput"
        app:layout_constraintEnd_toStartOf="@+id/namainput"
        app:layout_constraintTop_toTopOf="@+id/namainput"
        app:srcCompat="@drawable/ic_group_black_24dp" />

    <EditText
        android:id="@+id/tanggallahirinput"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Tanggal Lahir"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toTopOf="@+id/jeniskelamininput"
        app:layout_constraintEnd_toEndOf="@+id/jeniskelamininput"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView18" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/jeniskelamininput"
        app:layout_constraintEnd_toStartOf="@+id/jeniskelamininput"
        app:layout_constraintTop_toTopOf="@+id/jeniskelamininput"
        app:srcCompat="@drawable/ic_person_black_24dp" />

    <EditText
        android:id="@+id/namainput"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nama"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toTopOf="@+id/tanggallahirinput"
        app:layout_constraintEnd_toEndOf="@+id/tanggallahirinput"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView17" />

    <EditText
        android:id="@+id/nomorinput"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="nomorinput"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toTopOf="@+id/namainput"
        app:layout_constraintEnd_toEndOf="@+id/namainput"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/namainput" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/alamatinput"
        app:layout_constraintEnd_toStartOf="@+id/alamatinput"
        app:layout_constraintTop_toTopOf="@+id/alamatinput"
        app:srcCompat="@drawable/ic_place_black_24dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/tanggallahirinput"
        app:layout_constraintEnd_toStartOf="@+id/tanggallahirinput"
        app:layout_constraintTop_toTopOf="@+id/tanggallahirinput"
        app:srcCompat="@drawable/ic_date_range_black_24dp" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="@+id/nomorinput"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="@+id/nomorinput"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/student1" />

    <ImageView
        android:id="@+id/imagesave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/saveinput"
        app:layout_constraintEnd_toStartOf="@+id/saveinput"
        app:layout_constraintTop_toTopOf="@+id/saveinput"
        app:srcCompat="@drawable/ic_save_black_24dp" />

    <TextView
        android:id="@+id/saveinput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="92dp"
        android:text="SIMPAN"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>